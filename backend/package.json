{
  "name": "backend",
  "version": "1.0.0",
  "description": "LedgerLY Backend API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "dev:https": "HTTPS_PORT=3443 nodemon src/server.ts",
    "setup:https": "node scripts/generate-ssl-certs.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:setup": "cross-env NODE_ENV=test ts-node scripts/test-db-setup.ts",
    "test:db:reset": "cross-env NODE_ENV=test ts-node scripts/test-db-setup.ts",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/.*\\.test\\.ts$",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/integration/.*\\.test\\.ts$",
    "test:e2e": "cross-env NODE_ENV=test jest --testPathPattern=__tests__/e2e/.*\\.test\\.ts$",
    "test:scheduler": "cross-env NODE_ENV=test ts-node scripts/test-scheduler.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/node-cron": "^3.0.11",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "cron": "^4.3.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.0.5",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "plaid": "^33.0.0",
    "winston": "^3.17.0",
    "winston-cloudwatch": "^6.3.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.17.3",
    "csurf": "^1.11.0",
    "express-validator": "^7.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/cron": "^2.0.1",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.18",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^2.0.12",
    "@types/express-session": "^1.17.7",
    "@types/csurf": "^1.11.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.0",
    "prettier": "^3.4.2",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "wait-on": "^8.0.2"
  }
}
